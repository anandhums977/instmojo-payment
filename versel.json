{
    "version": 2,
    "builds": [
      {
        "src": "instamojo_payment/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "pythonVersion": "3.9",
            "maxLambdaSize": "15mb"
          }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "instamojo_payment/wsgi.py"
      }
    ]
  }
  